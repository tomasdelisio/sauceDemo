package utils;

public enum MessagesCart {
	//*** MESSAGES ***//
	// Login
	CART("INICIANDO CARRITO"),
	CART_COMPLETED("CARRITO COMPLETADO"),
	VALIDATE_CART("VALIDANDO CARRITO"),
	LOGIN_OK("CARRITO EXITOSO"),
	
	// Error
	LOGIN_BTN_NOT_VISIBLE("EL BOTÃ“N DE LOGIN NO ES VISIBLE");
	
	/*** VARIABLES ***/
	private final String message;
	
	/*** METHODS ***/
	// Setter
	MessagesCart(String message){
		this.message = message;
		
	}
	
	// Getter
	public String getMessage() {
		return message;
		
	}
}
